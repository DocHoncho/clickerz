<div role="tabpanel" class="tab-pane{% if this.active %} active{% endif %}" id="{{ this.name }}">
	<div class="panel panel-primary{% if this.attrs.class %} {{ this.attrs.class }}{% endif %}">
		<div class="panel-heading"><h2>{{ this.title }}</h2></div>
		<div class="panel-body">
		</div> <!-- /.panel-body -->
	</div> <!-- /.panel.panel-primary -->
</div>
